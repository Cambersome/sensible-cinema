<% edits = db_url.tags_by_type %>
{
"_comment": "generated at <%= Time.now %>.",
"url": <%= db_url.to_json %>,
"expected_url_unescaped": <%= HTML.unescape(db_url.url).to_json %>, <% # no way to unescape or stably escape in javascript :? %>
"tags":
  [<%= db_url.tags.map(&.to_json).join(",\n") %>],
"tag_edit_lists":
  <%= db_url.tag_edit_lists(env).map{|tag_edit_list| [tag_edit_list, tag_edit_list.tags_with_selected_or_not.map{|tag, action| tag.default_action = action; tag}]}.to_json %>
}
