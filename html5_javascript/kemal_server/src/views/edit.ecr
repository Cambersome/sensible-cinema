<h1>Editing <%= h db_url.url %></h1>

<form action="/save?url=<%= h db_url.url %>" method="POST">
<div>
<textarea name="name" rows="40" cols="40" style="font-size: 12pt; font-family: Arial;"><%= db_url.name %></textarea>
<textarea name="url" rows="40" cols="40" style="font-size: 12pt; font-family: Arial;"><%= db_url.url %></textarea>
</div>

<div><input type="submit" value="Save"></div>
</form>
<p>[<a href="/">index</a>]</p>
