{
  "manifest_version": 2,

  "name": "Clean stream plugin",
  "description": "This extension injects the clean stream player to any online video",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!" // tooltip
  },
  "permissions": [
    "<all_urls>", "tabs", "http://*/", "https://*/"
  ],
//  "content_scripts": ["contentscript.js"],
  "content_security_policy": "script-src 'self' https://rawgit.com https://*.rawgit.com; object-src 'self'",
  "web_accessible_resources": ["load_editor.js", "bootloader_dev.js", "injected.js"]
}

